# MySQL to BigQuery Pipeline Configuration

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=your_database_name
MYSQL_TABLE=your_table_name
MYSQL_USERNAME=your_mysql_username
MYSQL_PASSWORD=your_mysql_password

# BigQuery Configuration
BIGQUERY_TABLE=your-project:your_dataset.your_table
BIGQUERY_SCHEMA=  # Optional: JSON schema or file path

# GCP Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Dataflow Configuration
DATAFLOW_TEMP_LOCATION=gs://your-bucket/temp/
DATAFLOW_STAGING_LOCATION=gs://your-bucket/staging/

# Pipeline Configuration
BATCH_SIZE=10000
QUERY=  # Optional: Custom SQL query

# Python Configuration
PYTHONPATH=./src

# Optional: Logging Level
LOGGING_LEVEL=INFO

# Development Settings
ENVIRONMENT=development
